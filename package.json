{
  "name": "class-stores-plugin",
  "version": "0.0.3",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "watch": "rimraf dist && rollup -c rollup.config.js -w"
  },
  "main": "dist/class-stores-plugin.cjs.js",
  "module": "dist/class-stores-plugin.esm-bundler.js",
  "unpkg": "dist/class-stores-plugin.global.js",
  "jsdelivr": "dist/class-stores-plugin.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/class-stores-plugin.cjs.js",
      "browser": "./dist/class-stores-plugin.esm-browser.js",
      "import": "./dist/class-stores-plugin.esm-bundler.js",
      "module": "./dist/class-stores-plugin.esm-bundler.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "dependencies": {
    "@types/lodash": "^4.14.178",
    "@vue/devtools-api": "^6.0.0-beta.20.1",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/node": "^14.14.22",
    "@vue/compiler-sfc": "^3.2.26",
    "pascalcase": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.5.4",
    "vue": "^2.6.14"
  }
}
